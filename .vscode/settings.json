{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/*.js.map": true,
        "**/.vscode": true,
        "tsconfig.json": true,
        "package.json": true,
        "package-lock.json": true,
        ".gitignore": true,
        
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.d.ts, ${capture}.js, ${capture}.js.map"
    }
}